!
router bgp 31
 no bgp enforce-first-as
 bgp log-neighbor-changes
 neighbor 3.0.31.1 remote-as 3
 neighbor 3.0.31.1 description AS3-R1
 neighbor 2001:3:31::1 remote-as 3
 neighbor 2001:3:31::1 description AS3-R1
 neighbor 2001:123:123::254 remote-as 123
 neighbor 2001:123:123::254 description IX-AS123
 neighbor 123.123.0.254 remote-as 123
 neighbor 123.123.0.254 description IX-AS123
 !
 address-family ipv4
  network 31.31.11.0 mask 255.255.255.0
  network 31.31.12.0 mask 255.255.255.0
  network 31.31.13.0 mask 255.255.255.0
  redistribute connected route-map RM-CONNECTED2BGP-IPV4
  neighbor 3.0.31.1 activate
  neighbor 3.0.31.1 send-community both
  neighbor 3.0.31.1 route-map RM-AS31-TO-AS3-IPV4-OUT out
  no neighbor 2001:3:31::1 activate
  no neighbor 2001:123:123::254 activate
  neighbor 123.123.0.254 activate
  neighbor 123.123.0.254 send-community both
  neighbor 123.123.0.254 route-map RM-AS31-TO-IXAS123-IPV4-IN in
  neighbor 123.123.0.254 route-map RM-AS31-TO-IXAS123-IPV4-OUT out
 exit-address-family
 !
 address-family ipv6
  redistribute connected route-map RM-CONNECTED2BGP-IPV6
  network 2001:31::/32
  network 2001:31:31:11::/64
  network 2001:31:31:12::/64
  network 2001:31:31:13::/64
  neighbor 2001:3:31::1 activate
  neighbor 2001:3:31::1 send-community both
  neighbor 2001:3:31::1 route-map RM-AS31-TO-AS3-IPV6-OUT out
  neighbor 2001:123:123::254 activate
  neighbor 2001:123:123::254 send-community both
  neighbor 2001:123:123::254 route-map RM-AS31-TO-IXAS123-IPV6-IN in
  neighbor 2001:123:123::254 route-map RM-AS31-TO-IXAS123-IPV6-OUT out
 exit-address-family
!
ip forward-protocol nd
!
ip bgp-community new-format
ip as-path access-list 1 permit ^$
!
no ip http server
no ip http secure-server
!
!
ip prefix-list PL-AS31-31-31-11-0 seq 5 permit 31.31.11.0/24
!
ip prefix-list PL-AS31-IPV4 seq 5 permit 31.31.0.0/16 le 24
!
ip prefix-list PL-CONNECTED2BGP-IPV4 seq 5 permit 31.31.14.0/24
ip prefix-list PL-CONNECTED2BGP-IPV4 seq 10 permit 31.31.15.0/24
ip prefix-list PL-CONNECTED2BGP-IPV4 seq 15 permit 31.31.16.0/24
ipv6 route 2001:31::/32 Null0 name ISP31-AS31-CIDR
!
!
ipv6 prefix-list PL-AS31-IPV6 seq 5 permit 2001:31::/32 le 48
!
ipv6 prefix-list PL-CONNECTED2BGP-IPV6 seq 5 permit 2001:31:31:14::/64
ipv6 prefix-list PL-CONNECTED2BGP-IPV6 seq 10 permit 2001:31:31:15::/64
ipv6 prefix-list PL-CONNECTED2BGP-IPV6 seq 15 permit 2001:31:31:16::/64
route-map RM-AS31-TO-AS3-IPV4-OUT permit 10
 match ip address prefix-list PL-AS31-31-31-11-0
 match as-path 1
 set community 2:5012 additive
!
route-map RM-AS31-TO-AS3-IPV4-OUT permit 20
 match ip address prefix-list PL-AS31-IPV4
 match as-path 1
!
route-map RM-AS31-TO-AS3-IPV6-OUT permit 10
 match as-path 1
 match ipv6 address prefix-list PL-AS31-IPV6
!
route-map RM-AS31-TO-IXAS123-IPV6-OUT permit 10
 match as-path 1
 match ipv6 address prefix-list PL-AS31-IPV6
!
route-map RM-AS31-TO-IXAS123-IPV4-OUT permit 10
 match ip address prefix-list PL-AS31-IPV4
 match as-path 1
!
route-map RM-CONNECTED2BGP-IPV4 permit 10
 match ip address prefix-list PL-CONNECTED2BGP-IPV4
!
route-map RM-CONNECTED2BGP-IPV6 permit 10
 match ipv6 address prefix-list PL-CONNECTED2BGP-IPV6
!
route-map RM-AS31-TO-IXAS123-IPV6-IN permit 10
 match ipv6 address prefix-list ALL-ROUTES-IPV6
!
route-map RM-AS31-TO-IXAS123-IPV4-IN permit 10
 match ip address prefix-list ALL-ROUTES-IPV4
